local network = game:GetService("ReplicatedStorage"):WaitForChild("Network")
local upgradeEvent = network:WaitForChild("Upgrades_Purchase")

for _, upgrade in ipairs(upgrades) do
    local args = {
        [1] = upgrade
    }
    upgradeEvent:InvokeServer(unpack(args))
end
