local function invokeUpgrade(upgrade)
    local args = { upgrade }
    game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Upgrades_Purchase"):InvokeServer(unpack(args))
end

-- Loop through the list and invoke each upgrade
for _, upgrade in ipairs(upgrades) do
    invokeUpgrade(upgrade)
end
